{
  "name": "text-classification-extension",
  "version": "1.0.0",
  "description": "An extension created using the PRG AI Blocks framework",
  "main": "index.ts",
  "scripts": {
    "directory": "echo textClassification",
    "test": "pnpm --filter prg-extension-root test textClassification/index.test.ts",
    "dev": "pnpm --filter prg-extension-root dev --include textClassification",
    "add:ui": "pnpm --filter prg-extension-root add:ui textClassification",
    "add:arg": "pnpm --filter prg-extension-root add:arg textClassification"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@tensorflow-models/toxicity": "1.2.2",
    "@tensorflow-models/universal-sentence-encoder": "1.2.2",
    "@tensorflow/tfjs": "^1.5.1",
    "format-message": "^6.2.4",
    "sentiment": "^5.0.2"
  },
  "devDependencies": {
    "@types/dom-speech-recognition": "^0.0.1",
    "@types/sentiment": "^5.0.1"
  }
}