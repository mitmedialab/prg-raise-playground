{
  "name": "scratch-vm",
  "version": "1.2.4",
  "description": "Virtual Machine for Scratch 3.0",
  "author": "Massachusetts Institute of Technology",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/LLK/scratch-vm#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/LLK/scratch-vm.git",
    "sha": "4b4ca394b90ea1eb44391b67cf515e8b9e15205e"
  },
  "main": "./dist/node/scratch-vm.js",
  "browser": "./src/index.js",
  "scripts": {
    "test:jest": "node_modules/.bin/jest",
    "compile:ts": "ts-node ./scripts/transpile.ts",
    "build": "npm run docs && webpack --progress --colors --bail",
    "coverage": "tap ./test/{unit,integration}/*.js --coverage --coverage-report=lcov",
    "deploy": "touch playground/.nojekyll && gh-pages -t -d playground -m \"Build for $(git log -n1 --pretty=format:\"%h %s\") [skip ci]\"",
    "docs": "jsdoc -c .jsdoc.json",
    "i18n:src": "mkdirp translations/core && format-message extract --out-file translations/core/en.json src/extensions/**/index.js",
    "i18n:push": "tx-push-src scratch-editor extensions translations/core/en.json",
    "lint": "eslint . && format-message lint src/**/*.js",
    "prepare": "cd ../.. && husky install",
    "prepublish": "in-publish && npm run build || not-in-publish",
    "start": "webpack-dev-server",
    "tap": "tap ./test/{unit,integration}/*.js",
    "tap:unit": "tap ./test/unit/*.js",
    "tap:integration": "tap ./test/integration/*.js",
    "test": "npm run tap",
    "watch": "webpack --progress --colors --watch",
    "version": "json -f package.json -I -e \"this.repository.sha = '$(git log -n1 --pretty=format:%H)'\""
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "tap": {
    "branches": 60,
    "functions": 70,
    "lines": 70,
    "statements": 70
  },
  "dependencies": {
    "@teachablemachine/image": "^0.8.4",
    "@teachablemachine/pose": "^0.8.6",
    "@tensorflow-models/handpose": "0.0.3",
    "@tensorflow-models/knn-classifier": "1.2.2",
    "@tensorflow-models/posenet": "2.2.1",
    "@tensorflow-models/speech-commands": "0.4.2",
    "@tensorflow-models/toxicity": "1.2.2",
    "@tensorflow-models/universal-sentence-encoder": "1.2.2",
    "@tensorflow/tfjs": "^1.5.1",
    "@tensorflow/tfjs-layers": "^1.5.1",
    "@tensorflow/tfjs-node": "^1.5.1",
    "@vernier/godirect": "1.5.0",
    "arraybuffer-loader": "^1.0.6",
    "atob": "2.1.2",
    "btoa": "1.2.1",
    "canvas-toBlob": "1.0.0",
    "decode-html": "2.0.0",
    "diff-match-patch": "1.0.4",
    "format-message": "6.2.1",
    "format-message-parse": "^6.2.4",
    "htmlparser2": "3.10.0",
    "immutable": "3.8.1",
    "jszip": "^3.1.5",
    "mime-types": "^2.1.35",
    "minilog": "3.1.0",
    "nets": "3.2.0",
    "scratch-parser": "5.0.0",
    "scratch-sb1-converter": "0.2.7",
    "scratch-translate-extension-languages": "0.0.20191118205314",
    "sentiment": "latest",
    "socket.io-client": "2.0.4",
    "svelte": "3.52.0",
    "text-encoding": "0.7.0",
    "worker-loader": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@babel/preset-env": "7.14.8",
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@types/chokidar": "^2.1.3",
    "@types/glob": "^7.2.0",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.3",
    "@types/webgl2": "^0.0.6",
    "adm-zip": "0.4.11",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "callsite": "1.0.0",
    "chalk": "^4.0.1",
    "chokidar": "^3.5.3",
    "copy-webpack-plugin": "4.5.4",
    "docdash": "1.2.0",
    "eslint": "5.3.0",
    "eslint-config-scratch": "5.1.0",
    "expose-loader": "0.7.5",
    "file-loader": "2.0.0",
    "format-message-cli": "6.2.0",
    "gh-pages": "^1.2.0",
    "glob": "^8.0.3",
    "husky": "8.0.1",
    "i": "^0.3.7",
    "ignore-loader": "^0.1.2",
    "in-publish": "^2.0.0",
    "jest": "^28.1.2",
    "js-md5": "0.7.3",
    "jsdoc": "^3.6.6",
    "jsdom": "^20.0.0",
    "json": "^9.0.4",
    "lodash.defaultsdeep": "4.6.1",
    "npm": "^8.19.2",
    "pngjs": "3.3.3",
    "regenerator-runtime": "^0.13.9",
    "scratch-audio": "latest",
    "scratch-blocks": "latest",
    "scratch-l10n": "latest",
    "scratch-render": "latest",
    "scratch-render-fonts": "latest",
    "scratch-semantic-release-config": "1.0.4",
    "scratch-storage": "2.0.2",
    "scratch-svg-renderer": "latest",
    "script-loader": "0.7.2",
    "stats.js": "^0.17.0",
    "tap": "16.2.0",
    "tiny-worker": "2.3.0",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4",
    "uglifyjs-webpack-plugin": "1.2.7",
    "webpack": "4.46.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.2"
  },
  "overrides": {
    "@teachablemachine/image": {
      "@tensorflow/tfjs": "$@tensorflow/tfjs"
    },
    "@teachablemachine/pose": {
      "@tensorflow/tfjs": "$@tensorflow/tfjs"
    }
  }
}
